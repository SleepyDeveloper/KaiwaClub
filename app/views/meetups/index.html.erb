<div class="container">
  <h1>Meetups</h1>
  <table>
    <thead>
      <tr>
        <th>Date</th>
        <th>Show</th>
      </tr>
    </thead>
    <tbody>
      <% @meetups.each do | meetup | %>
      <tr>
        <td><%= meetup.date %></td>
        <td><%= link_to meetup.date, meetup %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>
